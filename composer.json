{
    "name": "medrv/filament-site-settings",
    "description": "Site settings plugin for FilamentPHP with light/dark logo, mail settings, and localization.",
    "type": "library",
    "autoload": {
        "psr-4": {
            "medrv\\SiteSettings\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "medrv\\SiteSettings\\SiteSettingsServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.1",
        "spatie/laravel-settings": "^2.0"
    }
}
